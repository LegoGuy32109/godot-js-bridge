	"version": "0.2.0",
	"configurations": [
		{
			"name": "GDScript: Launch Project",
			"type": "godot",
			"request": "launch",
			"project": "",
			"debug_collisions": false,
			"debug_paths": false,
			"debug_navigation": false,
			"additional_options": ""
		},
		{
			"name": "Debug Web Export",
			"type": "chrome",
			"request": "launch",
            "url": "https://localhost:8000",
            "webRoot": "${workspaceFolder}/exports",
            "preLaunchTask": "npx local-web-server",
            "postDebugTask": "stop server",
		}
	]
}
